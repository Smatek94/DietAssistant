<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.skolimowskim.dietassistant">

    <application
        android:name=".app.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.products.ProductsActivity">
        </activity>
        <activity android:name=".view.products.manage.ManageProductActivity">
        </activity>
        <activity android:name=".view.meals.MealsActivity">
        </activity>
        <activity android:name=".view.meals.manage.ManageMealActivity">
        </activity>
    </application>

</manifest>